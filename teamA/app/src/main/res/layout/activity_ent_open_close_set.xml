<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ent.EntOpenCloseSet">

    <ImageButton
        android:id="@+id/setOpenTime"
        android:layout_width="87dp"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="44dp" />

    <EditText
        android:id="@+id/openTimeView"
        android:layout_width="212dp"
        android:layout_height="62dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:text="개점시간"
        android:enabled="false"/>

    <ImageButton
        android:id="@+id/setCloseTime"
        android:layout_width="86dp"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="128dp" />

    <EditText
        android:id="@+id/closeTimeView"
        android:layout_width="212dp"
        android:layout_height="62dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="139dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:text="폐점시간"
        android:enabled="false"/>

    <ImageButton
        android:id="@+id/timeUpdate"
        android:layout_width="73dp"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="236dp"
        app:srcCompat="@android:drawable/stat_sys_upload_done" />

    <ImageButton
        android:id="@+id/timeCancel"
        android:layout_width="73dp"
        android:layout_height="68dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="237dp"
        android:layout_marginEnd="168dp"
        android:layout_marginRight="168dp"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel" />

    <ImageButton
        android:id="@+id/openClosePoint"
        android:layout_width="76dp"
        android:layout_height="71dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        app:srcCompat="@drawable/common_full_open_on_phone" />

</RelativeLayout>